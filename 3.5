library(quantmod)
library(ggplot2)
getSymbols("^OEX",src="yahoo")
OEX
head(OEX)
tail(OEX)
qplot(OEX[,4])
?rep


u<-rep(NA,length(OEX[,4]))
OEXclose<-OEX$OEX.Close

for (i in 2:length(OEX[,4])){
  u[1]<-log(OEXclose[[2]]/OEXclose[[1]])
  u[i]<-log(OEXclose[[i]]/OEXclose[[i-1]])
}
head(u)
logreturn<-c(NA,length(u)-1)
for (i in 1:length(u)-1){
  logreturn[i]<-u[i+1]
}
 head(logreturn) 
mean(logreturn)
var(logreturn)
sd(logreturn)
?qnorm
densitet<-dnorm(logreturn,mean(logreturn),sd(logreturn))
qplot(densitet, xlim = c(0,50))
